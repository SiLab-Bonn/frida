*******************general setting****************************************
.lib setting
    *.temp 27
    .model DIODE d
    .model NPN npn
    .model PNP pnp
    .include  'S:\technologies\tsl018\v2.0\v2.0.sp'
.endl

.lib setting_eldo
    *.temp 27
